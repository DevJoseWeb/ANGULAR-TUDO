{
  "name": "ngx-materialize",
  "version": "6.0.1",
  "description": "An Angular wrap around Materialize library",
  "license": "Apache-2.0",
  "repository": "sherweb/ngx-materialize",
  "keywords": [
    "angular",
    "ng",
    "ngx",
    "material",
    "material-design",
    "materialize",
    "materializecss",
    "component",
    "ui"
  ],
  "scripts": {
    "ng": "ng",
    "start": "yarn link:lib && ng serve",
    "build:demo": "ng build demo",
    "build:lib": "ng build lib",
    "postbuild:lib": "cpx ./README.md ./dist/lib",
    "link:lib": "rimraf node_modules/ngx-materialize && linklocal",
    "lint:ci": "ng lint lib --format checkstyle > tslint.xml",
    "lint:demo": "ng lint demo",
    "lint:lib": "ng lint lib",
    "test": "ng test",
    "postinstall": "typings install",
    "gh-pages:build": "yarn build:demo --prod --aot --base-href /ngx-materialize/",
    "gh-pages:deploy": "angular-cli-ghpages --dir=\"dist/demo\" --name=\"CircleCI\" --email=\"circleci@email.com\" --no-silent",
    "heroku-postbuild": "yarn install && yarn build:demo --prod --aot",
    "publish:lib": "npm publish ./dist/lib"
  },
  "dependencies": {
    "@angular/animations": "6.1.0",
    "@angular/common": "6.1.0",
    "@angular/compiler": "6.1.0",
    "@angular/core": "6.1.0",
    "@angular/forms": "6.1.0",
    "@angular/http": "6.1.0",
    "@angular/platform-browser": "6.1.0",
    "@angular/platform-browser-dynamic": "6.1.0",
    "@angular/router": "6.1.0",
    "@mdi/font": "^2.4.85",
    "core-js": "2.5.7",
    "highlightjs": "^9.10.0",
    "ngx-malihu-scrollbar": "^1.3.2",
    "ngx-markdown": "^6.1.0",
    "ngx-materialize": "file:./lib",
    "rxjs": "6.2.2",
    "web-animations-js": "^2.3.1",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.7.0",
    "@angular-devkit/build-ng-packagr": "~0.7.0",
    "@angular/cli": "6.1.0",
    "@angular/compiler-cli": "6.1.0",
    "@angular/language-service": "6.1.0",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "10.5.3",
    "angular-cli-ghpages": "^0.5.2",
    "codelyzer": "4.4.2",
    "cpx": "^1.5.0",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.5",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.2.0",
    "karma-junit-reporter": "^1.2.0",
    "linklocal": "^2.8.2",
    "ng-packagr": "^3.0.6",
    "rimraf": "^2.6.2",
    "ts-node": "7.0.0",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "5.11.0",
    "tslint-eslint-rules": "^5.2.0",
    "tslint-microsoft-contrib": "^5.0.3",
    "typescript": "2.9.2",
    "typings": "^2.0.0"
  }
}
